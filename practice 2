#include<stdio.h>


int main(){
    int ch;
    printf("enter character");
    scanf("%c",&ch);
    if(ch>='A' && ch<='Z'){
        printf("character %c");

    }
    else if(ch>='a' && ch<='z'){
        printf("small character %c");

    }
    else{
        printf("none");
    }
    return 0;
}

    
