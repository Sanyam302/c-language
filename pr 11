#include<stdio.h>

int main(){
    int n;
    printf("enter number");
    scanf("%d",&n);
    
    for(int j=1;j<=n;j++){
        for(int m=1;m<=n-j;m++){
            printf("a");
        }
        for(int i=1;i<=j ;i++){
            int a =1;
            int d = a+64;
            char ch = (char)d;

            printf("%c",d);
            a++;
        
        }printf("\n");

    }return 0;
}