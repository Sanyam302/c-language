#include<stdio.h>

int main(){
    int n,r,sum=0,temp;
    printf("enter number");
    scanf("%d",&n);
    temp=n;

    while(temp=n){
        r=n%10;
        sum = sum +(r*r*r);
        n=n/10;

    }
    if(sum=n){
        printf("armstrong");
    }
    else{
        printf("not");

    }
    return 0;
}